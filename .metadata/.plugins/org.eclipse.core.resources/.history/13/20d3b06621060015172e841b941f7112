package tests;

public class TestTest {
	
	public static void main(String[] args) {
		Robot aaron = new Robot("");
	}
	
	
}

class Robot {
	private String name;
	
	public Robot(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public void talk() {
		System.out.println("Beep-Beep Boop");
	}
}